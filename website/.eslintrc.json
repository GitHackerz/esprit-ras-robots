{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["warn", {
      "tabWidth": 4,
      "singleQuote": true,
      "semi": false,
      "trailingComma": "none",
      "arrowParens": "avoid",
      "endOfLine": "auto"
    }]
  }
}
